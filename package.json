{
  "name": "sagemaker-pipeline-example",
  "version": "0.1.0",
  "bin": {
    "sagemaker-pipeline-example": "bin/sagemaker-pipeline-example.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "visualize-combined": "cdk-dia --target docs/architecture-diagram.png",
    "visualize-network": "cdk-dia --target docs/architecture-diagram-network.png --stacks NetworkStack",
    "visualize-storage": "cdk-dia --target docs/architecture-diagram-storage.png --stacks StorageStack",
    "visualize-sagemaker": "cdk-dia --target docs/architecture-diagram-sagemaker.png --stacks SagemakerPipelineStack",
    "visualize": "npm run visualize-combined && npm run visualize-network && npm run visualize-storage && npm run visualize-sagemaker"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1027.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.211.0",
    "cdk-dia": "^0.12.0",
    "constructs": "^10.0.0"
  }
}
